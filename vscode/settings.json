        "editor.formatOnSave": true
    },
    "git.autofetch": true,
    "terminal.explorerKind": "external",
    "terminal.integrated.shell.osx": "/Users/neil.haggerty/code/vscode-terminal.sh",
    "terraform-ls.experimentalFeatures": {
      "validateOnSave": false
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "atlascode.bitbucket.pipelines.explorerEnabled": false,
    "atlascode.bitbucket.issues.explorerEnabled": false,
    "atlascode.helpExplorerEnabled": false,
    "python.linting.flake8Enabled": true,
    "workbench.colorTheme": "One Dark Pro"
